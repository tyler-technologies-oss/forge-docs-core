{
  "compilerOptions": {
    "target": "es5",
    "module": "es2015",
    "declaration": true,
    "strict": true, 
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react",
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node"
  },
  "include": ["src/**/*"]
}
